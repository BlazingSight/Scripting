    Noclip = function()
        noclip = game:GetService("RunService").Stepped:Connect(function()
         for i,v in pairs(char:GetDescendants()) do
             if v:IsA("BasePart") then 
                 v.CanCollide = false
             end
         end
        end)
     end

     
    for i,v in pairs(stats:GetChildren()) do
        if v:IsA("NumberValue") then    
            local label = Stat:Label(v.Name..": "..v.Value,{TextSize = 15, TextColor = Color3.fromRGB(255,255,255), BgColor = Color3.fromRGB(0,0,0)})
            v.Changed:Connect(function(val)
                label:Refresh(v.Name..": "..val,{TextSize = 15, TextColor = Color3.fromRGB(255,255,255), BgColor = Color3.fromRGB(0,0,0)})
            end)    
        end
    end

    local STR = false
    local del = 0
    local Farm = false
    Training:Toggle("Money Farm",function(bool)
    Farm = bool
    spawn(function()
        repeat wait()
            BV = true
            Noclip()
        if not humrp:FindFirstChild("BV") and BV then
            local BV2 = Instance.new("BodyVelocity",humrp)
            BV2.Name = "BV"
            BV2.MaxForce = Vector3.new(1e8,1e8,1e8)
            BV2.Velocity = Vector3.new(0,0,0)
            BV2.P = 1e8
        end
        if not stats.DoingQuest.Value then
        game:GetService("ReplicatedStorage").Functions.StartRandomQuest:FireServer()
        stats.DoingQuest.Changed:Wait(2)
        end
        
        if stats.DoingQuest.Value and Farm then
            for i,v in pairs(locations[stats.CurrentQuest.QuestType.Value]) do
                if v:FindFirstChild("QuestMarker") then
                    if v:FindFirstChild("TouchInterest") then
                       
                        if stats.CurrentQuest.QuestType.Value == "Delivery" then
                            wait(.2)
                            humrp.CFrame = v.CFrame*CFrame.new(0,-20,0)
                            wait(.2)
                            humrp.CFrame = v.CFrame*CFrame.new(0,-15,0)
                        else
                            wait(.2)
                            humrp.CFrame = v.CFrame*CFrame.new(0,-25,0)
                            wait(.2)
                            humrp.CFrame = v.CFrame*CFrame.new(0,-15,0)
                        end
                        
                        firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"],v,0)
                        firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"],v,1)
                    elseif v:FindFirstChild("CD") then 
                        humrp.CFrame = v:FindFirstChildOfClass("Part").CFrame*CFrame.new(0,-7,0)
                        wait(.2)
                        fireclickdetector(v.CD.ClickDetector)
                        firesignal(plr.PlayerGui:WaitForChild("ChatUI"):WaitForChild("Choices"):WaitForChild("ButtonTemplate").MouseButton1Click)
                    end
                end
            end
            end
           
        until not Farm
        humrp:FindFirstChild("BV"):Destroy()
        noclip:Disconnect()
    end)
    end)

    Training:Toggle("Auto Strength(stand near bag)",function(bool)
